pipeline {
    agent any 
    stages {
        stage ('stagename -1') {
            steps {
                sh "echo this is my first stage in the jenkins pipeline"
            }
        }

        stage ('stagename -2') {
            steps {
                sh "echo I am executing stage 2"
            }
        }

        stage ('stagename -3') {
            steps {
                sh "echo I am executing stage 3"
            }
        }
    }
}
